      <div class="coursee_detail_container">
        <div class="right">
            <h1><%= course.title %></h1>
            <p><%= course.description %></p>
            <div class="profile">
                <h3>Created by</h3>
                <div class="img-t">
                    <img src="/images/<%= course.instructor_image %>" alt="">
                </div>
                <h4><%= course.instructor_name %></h4>
            </div>

            <div class="stars">
                <% for (let i = 0; i < Math.floor(course.rating); i++) { %>
                    <i class="fa-solid fa-star"></i>
                <% } %>
                <% if (course.rating % 1 !== 0) { %>
                    <i class="fa-solid fa-star-half-stroke"></i>
                <% } %>
            </div>

            <hr>

            <div class="what-u-learn">
                <h2>What you'll learn</h2>
                <p><%= course.what_you_learn %></p>
            </div>
        </div>

        <div class="left">
            <div class="copo">
                <div class="img-co">
                    <img src="/images/<%= course.image %>" alt="">
                </div>

                                

                <div class="bool">

                    <button><a href="#" id="now">Pay Now</a></button>
                    <button><a href="/register" id="re">Pay Later</a></button>
                </div>


                <div class="duration">
                    <h2>Duration :</h2>
                    <p><%= course.duration_weeks %> weeks</p>
                </div>

                <div class="Certificate">
                    <i class="fa-solid fa-certificate"></i>
                    <p>Certificate of completion</p>
                    
                </div>
            </div>
        </div>
