<div class="Registeration-container" >
      <div class="register">
            <form action="/save" method="post">
                <h2>Registration</h2>
                
                <div class="form-row">
                    <div class="form-group">
                        <label for="Name">Full Name</label>  <br>
                        <input type="text" name="Name" id="Name" class="form-control" placeholder="Enter your Name" required>
                    </div>

                    <div class="form-group">
                        <label for="Gender">Gender</label>
                        <br>
                        <select name="Gender" id="Gender" class="form-control">
                            <option value="" selected disabled>Select your gender</option>
                            <option value="Male">Male</option>
                            <option value="Female">Female</option>
                        </select>
                    </div> 
                </div>

                <div class="form-row">
                    <div class="form-group">
                        <label for="Phone">Phone</label>  <br>
                        <input type="number" name="Phone" id="Phone" class="form-control" placeholder="Enter your Phone" required>
                    </div>

                    <div class="form-group">
                        <label for="Courses">Courses</label>  <br>
                        <select name="Courses" id="Courses" class="form-control">
                            <option value="" selected disabled>Select Course</option>
                            <option value="Networking">Networking</option>
                            <option value="Fullstack Web">Fullstack Web</option>
                        </select>
                    </div>
                </div>

                <div class="form-row">
                    <div class="form-group">
                        <label for="City">City</label>  <br>
                        <input type="text" name="City" id="City" class="form-control" placeholder="Enter your City" required>
                    </div>
                    <div class="form-group">
                        <label for="Address">Address</label>  <br>
                        <input type="text" id="Address" name="Address" class="form-control" placeholder="Enter your address...">
                    </div>
                </div>

                <div class="form-row">
                    <div class="form-group">
                        <label for="Email">Email</label>  <br>
                        <input type="email" class="form-control" id="Email" placeholder="Enter email" name="Email" required>
                    </div>

                    <div class="form-group">
                        <label for="Date">Date</label>  <br>
                        <input type="date" name="Date" id="Date" class="form-control" placeholder="Select your date">
                    </div>
                </div>

                <button type="submit" class="btn btn-success" style="width: 100%;">Submit</button>
            </form>
        </div> 
